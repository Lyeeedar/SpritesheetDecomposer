<Application
	x:Class="SpritesheetDecomposer.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="clr-namespace:SpritesheetDecomposer"
	xmlns:v="clr-namespace:SpritesheetDecomposer.View"
	StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<v:SharedResourceDictionary
					Source="View/Styles/Brushes.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/GeneralStyles.xaml" />

				<v:SharedResourceDictionary
					Source="View/Styles/Button.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/CheckBox.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/ComboBox.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/Menu.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/Scrollbar.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/Slider.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/TabControl.xaml" />
				<v:SharedResourceDictionary
					Source="View/Styles/TextBox.xaml" />

			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
